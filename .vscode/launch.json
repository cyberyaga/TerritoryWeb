{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch and Debug Hosted Blazor WebAssembly App",
            "type": "blazorwasm",
            "request": "launch",
            "browser": "edge",
            "hosted": true,
            // If you have changed target frameworks, make sure to update the program path.
